{
  "name": "workflow",
  "root": "apps/workflow",
  "sourceRoot": "apps/workflow/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/workflow"
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "workflow:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/workflow"],
      "options": {
        "jestConfig": "apps/workflow/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/workflow/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": ["scope:workflow", "type:app"]
}