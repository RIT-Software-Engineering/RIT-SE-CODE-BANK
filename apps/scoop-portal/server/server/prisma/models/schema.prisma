generator client {
  provider = "prisma-client-js"
  output   = "../../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

//example data model
model fruit {
  id     String @id @default(uuid())
  name   String
  color  String
  size   String
  rating String @default("0")
}
