{
  "name": "peereval",
  "root": "apps/peereval",
  "sourceRoot": "apps/peereval/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/peereval"
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "peereval:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/peereval"],
      "options": {
        "jestConfig": "apps/peereval/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/peereval/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": ["scope:peereval", "type:app"]
}