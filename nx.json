{
  "npmScope": "RIT-SE-CODE-BANK",
  "installation": {
    "version": "21.2.0"
  },
  "defaultBase": "main",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nx/workspace/tasks-runners/default",
      "options": {
        "runtimeCacheInputs": [
          "node -v"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{options.outputPath}"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "default",
        "^production"
      ],
      "cache": true
    },
    "lint": {
      "inputs": [
        "default"
      ],
      "cache": true
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {}
}
